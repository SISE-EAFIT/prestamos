<%= form_for(@lend, html: { class: "form-horizontal", role: "form" }) do |f| %>
  <% if @lend.errors.any? %>
    <div class="alert alert-danger alert-dismissable" role="alert">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <h4><%= pluralize(@lend.errors.count, "error") %> prohibited this lend from being saved:</h4>

      <ul>
      <% @lend.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :date, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.date_select :date, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :quantity, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.number_field :quantity, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :student_id, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :student_id, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :element_id, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :element_id, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
