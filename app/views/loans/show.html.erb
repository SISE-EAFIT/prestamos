<div class="page-header">
  <%= link_to loans_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_loan_path(@loan), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show loan</h1>
</div>

<dl class="dl-horizontal">
  <dt>Date:</dt>
  <dd><%= @loan.date %></dd>

  <dt>Quantity:</dt>
  <dd><%= @loan.quantity %></dd>

  <dt>Student:</dt>
  <dd><%= @loan.student %></dd>

  <dt>Element:</dt>
  <dd><%= @loan.element %></dd>

</dl>
